	<test strategy="TOKENIZE_STEP_INSTANCE">
		<actual>hello the «world </actual>
		<expected>
			<token>hello</token>
			<token type="separator"> </token>
			<token>the</token>
			<token type="separator"> «</token>
			<token>world </token>
		</expected>
	</test>
	<test strategy="TOKENIZE_STEP_INSTANCE">
		<actual>"«</actual>
		<expected>
			<token type="separator">"</token>
			<token>«</token>
		</expected>
	</test>
	<test strategy="TOKENIZE_STEP_INSTANCE">
		<actual>"", ""</actual>
		<expected>
			<token type="separator">"</token>
			<token></token>
			<token type="separator">", "</token>
			<token></token>
			<token type="separator">"</token>
		</expected>
	</test>
</suite>
